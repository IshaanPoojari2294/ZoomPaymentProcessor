{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "description": "A template express server for Zoom Apps",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts",
    "start-dev": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts",
    "build": "tsc -b",
    "dev": "tsc-watch --outDir ../dist --noClear --onSuccess \"npm run start-dev\""
  },
  "dependencies": {
    "axios": "^0.26.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.6",
    "cookie-session": "^2.0.0",
    "debug": "~4.3.3",
    "dotenv": "^16.0.0",
    "express": "~4.17.3",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "morgan": "~1.10.0",
    "pug": "^3.0.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.44",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/websocket": "^1.0.5",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.4.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "tsc-watch": "^5.0.2",
    "typescript": "^4.6.3"
  }
}
